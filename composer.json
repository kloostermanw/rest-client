{
    "name": "sc0/rest-client",
    "description": "Simple PHP Rest Client",
    "keywords": ["rest", "rest-client"],
    "homepage": "https://github.com/sc0rp10/rest-client",
    "type": "library",
    "require": {
        "guzzlehttp/guzzle": "^7.0",
        "php": ">=8.1",
        "ext-libxml": "*",
        "ext-simplexml": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Konstantin Zamyakin",
            "email": "dev@weblab.pro"
        }
    ],
    "minimum-stability": "alpha",
    "autoload": {
        "psr-4": {
            "Sc\\RestClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sc\\Tests\\": "tests/"
        }
    }
}
